 #Check Presence on System. If present, uninstall. 
 
 $registryExists = Test-Path "PATH"     
       if ($registryExists) {
     
       $app = Get-WmiObject -Class Win32_Product `
                     -Filter "Name = 'SOFTWARE TITLE'" 
                     
                     
       $app.Uninstall()
       }
     
       Else {}
