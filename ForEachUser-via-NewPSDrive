 #New-PSDrive HKU Registry HKEY_USERS
 
        $profiles = Get-ChildItem "HKU:\${sid}"
        $acct = $acct.pschildname
        
            foreach ($acct in $profiles)
                    {
                    #Action Items:
                    New-Item -Path "HKU:\$acct\Software\Microsoft\Windows\CurrentVersion\Explorer\FileExts\.pdf\" -Name NAME -Force
                    Get-Item "HKU:\$acct\Software\Microsoft\Windows\CurrentVersion\Explorer\FileExts\.pdf\UserChoice\" | New-ItemProperty -Name Progid -Value APPVALUE -Force
                    }
                    
            Remove-Variable -Name profiles
            Remove-Variable -Name acct
            Remove-PSDrive -Name HKU
